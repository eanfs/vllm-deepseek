# ============================================
# DeepSeek-OCR vLLM 部署配置（ModelScope 版本）
# ============================================
# 复制此文件为 .env 并填写你的配置:
#   cp .env.example .env

# ModelScope 模型目录（下载后的本地路径）
MODELSCOPE_MODEL_DIR=./models/deepseek-ai/DeepSeek-OCR-2

# ModelScope 缓存目录
MODELSCOPE_CACHE_DIR=./models

# vLLM 镜像版本
VLLM_IMAGE_TAG=latest

# 暴露的主机端口
HOST_PORT=8000

# GPU 数量
GPU_COUNT=all

# 模型最大序列长度
MAX_MODEL_LEN=8192

# GPU 显存使用率
GPU_MEMORY_UTILIZATION=0.90

# vLLM 日志级别
VLLM_LOGGING_LEVEL=INFO
